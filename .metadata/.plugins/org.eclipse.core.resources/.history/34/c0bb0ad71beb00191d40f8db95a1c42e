package com.day2;

public class Flip {
	


	public static void main(String [] args)
	
	{
		
	int	counter=0;
	int head=0;
	int tail=0;
	
	
	
		while(Math.random()<0.5)
		{
			
			System.out.println("head:"+head);
			counter++;
			System.out.println("counter:"+counter);
		}
		else
			
		{
			
			System.out.println("tail:"+tail);
			counter++;
			System.out.println("counter:"+counter);
			
		}
		
	}

}
