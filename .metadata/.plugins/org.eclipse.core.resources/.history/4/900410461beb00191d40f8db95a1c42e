package com.day2;

public class Flip {
	


	public static void main(String [] args)
	
	{
		
	int	counter=0;
	int head;
	int tail;
	
	
	
		if(Math.random()<0.5)
		{
			counter++;
			System.out.println("head:"+head);
			
		}
		else
			
		{
			counter++;
			System.out.println("tail:"+tail);
		}
		
	}

}
