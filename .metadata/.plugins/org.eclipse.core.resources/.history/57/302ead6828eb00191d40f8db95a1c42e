package com.day2;

import java.util.Scanner;

public class prime {
	
	public static void main(String [] args) 
	{
	
   Scanner sc =new Scanner(System.in);
   System.out.println("Enter numbers ");
   int num=sc.nextInt();
   
   int temp=0;
   
   for(int i=0; i<=num; i++)
   {
	   for(int j=2;j<i-1;j++)
	   {
		   
		temp++;  
	   }
	   if(temp!=0)
	   {
		   System.out.println("number is prime ");
	    }
	   else
	   {
		   System.out.println("number is not prime");
	   }
	   
   }
}
}
